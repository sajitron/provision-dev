# shellcheck shell=bash

# Make sure we have the latest changes (if on main)
if [[ "main" = $(git symbolic-ref --quiet --short HEAD 2>/dev/null || git rev-parse --short HEAD 2>/dev/null) ]]; then
    git pull --autostash
else
    printf "\nWARNING: You are not currently on main branch.\n"
fi

# SKIP_PROVISIONER="$SKIP_PROVISIONER" zsh ./script/zsh/init.sh && . ~/.zshrc
